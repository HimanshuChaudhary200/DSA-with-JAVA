//Unary operators
import java.util.*;
public class JavaRevision{
    public static void main(String args[]){
        //Pre increment
        int a=7;
        int b=++a;

        System.out.println("a: "+a);
        System.out.println("b: "+b);

        //post increment

        int c=9;
        int d=c++;

        System.out.println("c: "+c );
        System.out.println("d: "+d);
        
    
    }
}
