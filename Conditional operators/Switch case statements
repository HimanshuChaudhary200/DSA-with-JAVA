 //Switch case statements
import java.util.*;
public class JavaRevision{
    public static void main(String args[]){
        int number=2;
        switch (number) {
            case 1: System.out.println("Burgur");
                
            break;

            case 2:  System.out.println("Chhole bhature");
            break;   

            case 3: System.out.println("Samosa");
        
            default:System.out.println("It was a dream");
                break;
        }
       
    }
}
