//Bit manipulation
//Bitwise operator

import java.util.*;
public class DataStructure{


        

    public static void main(String[] args) {

        //Binary AND operator
        System.out.println(5&7);

        //Binary OR operator
        System.out.println(5|7);

        //Binary XOR operator
        System.out.println(5^7);

        //Binary 1's implememnt
        System.out.println((~5));

        // Binary left shift
        System.out.println(5<<6);

        //Binary right shift
        System.out.println(5>>6);
                       
    }
}
