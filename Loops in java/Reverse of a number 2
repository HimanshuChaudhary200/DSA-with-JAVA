  //Print the reverse of a number 
import java.util.*;
public class JavaRevision{
    public static void main(String args[]){
        int n=456327;
        int rev=0;
        while(n>0){
            int lastdigit=n%10;
            rev=(rev*10)+lastdigit;
            n=n/10;
            
        }
        System.out.println(rev);
        // System.out.println();
        
    }
          
}  
